#初始化
{
	delayTime = 0.1
	Sprite.create("background", "image")
}
:Init

#:CreateSprite name="background" type=image
:AddBehaviour name="background" behaviour="AnimFadeIn" time=10

==Begin==

:SetSprite name="background" image="Logo.png"
:Wait time={return delayTime}

{
	s = Sprite.get("background")
	if (s.alpha > 0.5) then
		Script.jump("Begin")
	end
}

:SetSprite name="background" image=""
:Wait time={return delayTime}

:Goto pos="Begin"

==Exit==
